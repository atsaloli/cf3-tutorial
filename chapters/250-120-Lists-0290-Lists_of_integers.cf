bundle agent main {

  vars:

      "my_list"

        handle => "int_list",
        comment => "Demonstrate a list of integers",
        ilist => { "1", "2", "3" };
  reports:



      "Iterating over the values in @(my_list):  $(my_list)"
        handle => "demo_int_list",
        comment => "Demonstrate implicit looping over an ilist";
}

