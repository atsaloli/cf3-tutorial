# Examples of scalar variables

bundle agent main {

  vars:

      "my_string"      string  => "String contents...";

      "my_int" int => "42";

      "my_real" real    => "3.141592654";

      "my_second_string"
        string =>  "I love: $(my_string)";

  reports:

      "My string is: $(my_string)
My integer is: $(my_int)
My real number is: $(my_real)
$(my_second_string)";

}
