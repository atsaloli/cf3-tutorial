bundle agent main {

  vars:

      "shopping_list"

        slist   => {
                     "apples",
                     "bananas",
                     "grapes",
                     "coconuts",
                     "hamburgers",
                   };

      "second_list"
        slist => {
          "some item",
          @(shopping_list),
          "last item",
        };

  reports:

      "Buy $(second_list)";
}

