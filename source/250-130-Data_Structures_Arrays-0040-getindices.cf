# Add an slist containing the keys of the array

bundle agent main
{
  vars:
      "food_prices[Apple]"
        string =>  "58c";

      "food_prices[Banana]"
        string =>  "30c";

      "foods"
        slist => getindices("food_prices");

  reports:
      "Keys of 'food_prices' array: $(foods)";
}
