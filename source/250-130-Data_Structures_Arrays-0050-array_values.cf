# Use the keys to retrieve the values

bundle agent main
{
  vars:
      "food_prices[Apple]" string => "58c";
      "food_prices[Banana]" string => "30c";
      "food"
        slist => getindices("food_prices");

  reports:
      "Values of 'food_prices' array:  $(food): $(food_prices[$(food)])";
}
