bundle agent main {

  vars:

      "food_prices[Apple]"
        string =>  "59c";

      "food_prices[Banana]"
        string =>  "30c";

      "food_prices[numlist]"
        ilist => { "1", "2", "3" };


  reports:
      "Apple costs $(food_prices[Apple])";
      "Banana costs $(food_prices[Banana])";
      "Numlist: $(food_prices[numlist])";

}
